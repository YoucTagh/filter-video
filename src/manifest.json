{
  "manifest_version": 3,
  "name": "URL Logger & Video Blur",
  "version": "1.0",
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}